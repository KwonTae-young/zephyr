# Top-level configuration file for Industrial I/O drivers.

menuconfig IIO
	bool "Industrial I/O support"
	select HAS_IIO_DRIVERS
	help
	  Include Industrial I/O drivers in the system configuration.

if IIO

module = IIO
module-str = iio
source "subsys/logging/Kconfig.template.log_config"

config IIO_INIT_PRIORITY
	int "Industrial I/O initialization priority"
	default 90
	help
	  System initialization priority for Industrial I/O drivers.

source "drivers/iio/Kconfig.sn65hvs885"

endif # IIO
